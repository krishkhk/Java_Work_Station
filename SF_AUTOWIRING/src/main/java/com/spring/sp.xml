<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean class="com.spring.Student" id="student1">
<property name="sno" value="100"></property>
<property name="sname" value="Hari"></property>
<property name="sadd" value="Hyderabad"></property>
</bean>

<bean class="com.spring.College" id="clg" autowire="byType">
<property name="name" value="Ojas"></property>
</bean>
</beans>
